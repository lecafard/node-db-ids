{
  "compilerOptions": {
    "lib": ["ES2024"],
    "target": "ES2024",
    "module": "ES2022",
    "moduleDetection": "force",
    "declaration": true,

    "verbatimModuleSyntax": true,

    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "noFallthroughCasesInSwitch": true,

    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noPropertyAccessFromIndexSignature": false
  }
}
